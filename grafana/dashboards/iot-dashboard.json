  "templating": {
    "list": [
      {
        "current": {
          "selected": false,
          "text": "device_001",
          "value": "device_001"
        },
        "datasource": {
          "type": "influxdb",
          "uid": "${DS_INFLUXDB}"
        },
        "definition": "from(bucket: \"${INFLUX_BUCKET}")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"sensor_data\")\n  |> keep(columns: [\"device_id\"])\n  |> distinct(column: \"device_id\")\n  |> sort()\n  |> yield(name: \"device_ids\")",
        "hide": 0,
        "includeAll": false,
        "label": "Device",
        "multi": false,
        "name": "device",
        "query": "from(bucket: \"${INFLUX_BUCKET}")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"sensor_data\")\n  |> keep(columns: [\"device_id\"])\n  |> distinct(column: \"device_id\")\n  |> sort()\n  |> yield(name: \"device_ids\")",
